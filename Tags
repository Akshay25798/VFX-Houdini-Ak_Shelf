import os 
import hou
try:
    node = hou.pwd()
    path = node.evalParm("file")
    file = os.path.dirname(path)+"/Tag_Version.pub"
    if os.path.exists(file):
        with open(file,"r") as f:
            return(f.readline())
    if not os.path.exists(os.path.dirname(path)):
        return("current version is not cached")
except:
    return("current version is not cached")
